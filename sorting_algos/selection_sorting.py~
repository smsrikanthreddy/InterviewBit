
def selection_sort(A, n):
    for i in range(0, n-2):
        imin = i
        for j in range(i+1, n-1):
            if A[j] < A[imin]:
                imin = j
        temp = A[i]
        A[i] = A[imin]
        A[imin] = temp
    return A



A = [2,7,4,1,5,3]
n = len(A)-1
print('selection sorted list are:-', selection_sort(A, n))
